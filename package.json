{
  "name": "smart-storage-system",
  "version": "1.0.0",
  "description": "Smart Storage System with auto-schema generation and unified retrieval API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate:latest": "knex migrate:latest --knexfile src/config/knex.config.js",
    "migrate:rollback": "knex migrate:rollback --knexfile src/config/knex.config.js",
    "migrate:make": "knex migrate:make --knexfile src/config/knex.config.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "keywords": [
    "storage",
    "file-upload",
    "schema-generation",
    "postgres",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "file-type": "^18.7.0",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "mongoose": "^8.0.3",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1",
    "sharp": "^0.33.1",
    "fluent-ffmpeg": "^2.1.2",
    "ndjson": "^2.0.0",
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
